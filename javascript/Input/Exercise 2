const eventName = "Community Cleanup";
const eventDate = "2025-06-01";
let availableSeats = 25;

console.log(`Event: ${eventName}, Date: ${eventDate}, Seats: ${availableSeats}`);

function register() {
  if (availableSeats > 0) {
    availableSeats--;
    console.log(`Registered successfully! Seats left: ${availableSeats}`);
  } else {
    console.log("No seats available.");
  }
}
